{
  "name": "@ignatiusmb/sapper-app",
  "description": "Sapper app template",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "sapper dev",
    "build": "sapper build --legacy",
    "export": "run-s sapper:export static:404",
    "sapper:export": "sapper export --legacy",
    "static:404": "run-s static:404:mv static:404:rm",
    "static:404:mv": "mv __sapper__/export/404/index.html __sapper__/export/404.html",
    "static:404:rm": "rmdir \"__sapper__/export/404\"",
    "lint": "eslint src --ext svelte,js",
    "check": "prettier -c src",
    "format": "prettier -w src"
  },
  "engines": {
    "node": ">=12.x"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "polka": "^1.0.0-next.11",
    "sirv": "^1.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-object-assign": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-typescript": "^7.10.4",
    "@babel/runtime": "^7.11.2",
    "@ignatiusmb/aqua": "^20.3.1",
    "@ignatiusmb/elements": "0.4.2",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.2.0",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@rollup/plugin-typescript": "^5.0.2",
    "@types/express": "^4.17.7",
    "@types/node": "^14.6.0",
    "autoprefixer": "^9.8.6",
    "babel-jest": "^26.3.0",
    "eslint": "^7.7.0",
    "eslint-plugin-svelte3": "^2.7.3",
    "husky": "^4.2.5",
    "markdown-it": "^11.0.0",
    "markdown-it-container": "^3.0.0",
    "markdown-it-mark": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.32",
    "prettier": "^2.1.1",
    "prettier-plugin-svelte": "^1.1.1",
    "rollup": "^2.26.5",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "sapper": "0.28.10",
    "svelte": "^3.29.0",
    "svelte-preprocess": "^4.1.1",
    "tslib": "^2.0.1",
    "typescript": "^4.0.2"
  }
}
